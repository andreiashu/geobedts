{
  "name": "geobedts",
  "version": "1.0.0",
  "description": "High-performance offline geocoding library for TypeScript - port of Go geobed",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
